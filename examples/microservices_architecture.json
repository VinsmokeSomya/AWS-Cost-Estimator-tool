{
  "title": "Microservices Architecture",
  "description": "A scalable microservices architecture with service discovery and load balancing",
  "nodes": [
    {
      "id": "api_gateway",
      "type": "APIGateway",
      "label": "API Gateway",
      "CostBreakdown": {
        "CostPerRequest": 2000000
      },
      "Description": "Main entry point for all API requests"
    },
    {
      "id": "user_service",
      "type": "EC2",
      "label": "User Service",
      "instance_type": "t3.medium",
      "hours_per_month": 730,
      "Description": "Handles user authentication and management"
    },
    {
      "id": "order_service",
      "type": "EC2",
      "label": "Order Service",
      "instance_type": "t3.medium",
      "hours_per_month": 730,
      "Description": "Manages order processing and tracking"
    },
    {
      "id": "inventory_service",
      "type": "EC2",
      "label": "Inventory Service",
      "instance_type": "t3.medium",
      "hours_per_month": 730,
      "Description": "Handles inventory management"
    },
    {
      "id": "user_db",
      "type": "RDS",
      "label": "User Database",
      "instance_type": "db.t3.small",
      "storage_gb": 50,
      "hours_per_month": 730,
      "Description": "Stores user data and authentication info"
    },
    {
      "id": "order_db",
      "type": "RDS",
      "label": "Order Database",
      "instance_type": "db.t3.small",
      "storage_gb": 100,
      "hours_per_month": 730,
      "Description": "Stores order information"
    },
    {
      "id": "inventory_db",
      "type": "RDS",
      "label": "Inventory Database",
      "instance_type": "db.t3.small",
      "storage_gb": 200,
      "hours_per_month": 730,
      "Description": "Stores inventory data"
    },
    {
      "id": "cache_layer",
      "type": "ElastiCache",
      "label": "Redis Cache",
      "instance_type": "cache.t3.medium",
      "node_count": 2,
      "hours_per_month": 730,
      "Description": "Caches frequently accessed data"
    },
    {
      "id": "message_queue",
      "type": "SNS",
      "label": "Message Queue",
      "CostBreakdown": {
        "TopicCost": 1000000
      },
      "Description": "Handles inter-service communication"
    },
    {
      "id": "service_logs",
      "type": "S3",
      "label": "Service Logs",
      "Size": 200,
      "RequestCount": 500000,
      "Description": "Stores service logs and metrics"
    }
  ],
  "edges": [
    {"from": "api_gateway", "to": "user_service"},
    {"from": "api_gateway", "to": "order_service"},
    {"from": "api_gateway", "to": "inventory_service"},
    {"from": "user_service", "to": "user_db"},
    {"from": "order_service", "to": "order_db"},
    {"from": "inventory_service", "to": "inventory_db"},
    {"from": "user_service", "to": "cache_layer"},
    {"from": "order_service", "to": "cache_layer"},
    {"from": "inventory_service", "to": "cache_layer"},
    {"from": "user_service", "to": "message_queue"},
    {"from": "order_service", "to": "message_queue"},
    {"from": "inventory_service", "to": "message_queue"}
  ]
} 